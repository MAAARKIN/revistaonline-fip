<div>
	<label>Nome:</label>
	<input type="text" name="usuario.nome" placeholder="Nome" value="${usuario.nome}" />
</div>

<div>
	<label>Login:</label>
	<input type="text" name="usuario.login" placeholder="Login" value="${usuario.login}" />
</div>

<div>
	<label>Senha:</label>
	<input type="password" name="usuario.senha" placeholder="Senha" value="${usuario.senha}" />
</div>

<div>
	<label>Cpf:</label>
	<input type="text" name="usuario.cpf" placeholder="CPF" value="${usuario.cpf}" />
</div>

<div>
	<label>E-Mail:</label>
	<input type="text" name="usuario.email" placeholder="E-Mail" value="${usuario.email}" />
</div>
